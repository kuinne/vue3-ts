    }
  },
  "dependencies": {
    "@element-plus/icons-vue": "^2.1.0",
    "ant-design-vue": "^3.2.16",
    "dayjs": "^1.11.7",
    "dom-to-image": "^2.6.0",
    "echarts": "^5.4.2",
    "element-plus": "^2.3.1",
    "html2canvas": "^1.4.1",
    "jspdf": "^2.5.1",
    "loadsh": "^0.0.4",
    "lodash": "^4.17.21",
    "pinia": "^2.0.36",
    "vue": "^3.2.47",
    "vue-router": "4",
    "vue-webrtc": "^3.0.1",
    "vue3-draggable": "^2.0.9",
    "vue3-echarts": "^1.1.0",
    "vuedraggable": "^4.1.0",
    "webrtc-adapter": "^8.2.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.5.1",
